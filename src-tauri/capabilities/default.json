{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Permisos base para la app de Almacen",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "http:default",
    {
      "identifier": "http:allow-fetch",
      "allow": [
        {
          "url": "http://localhost:8080/**"
        },
        {
          "url": "http://127.0.0.1:8080/**"
        }
      ]
    },
    "sql:default"
  ]
}